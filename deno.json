  "tasks": {
    "crtb": "deno run -A main.ts",
    "compile": "deno compile -A --no-check --include docker/ --include templates/ --output crtb main.ts",
    "install": "deno run -A installer/install.ts",
    "test": "deno test -A"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1",
    "@std/path": "jsr:@std/path",
    "@std/fs": "jsr:@std/fs",
    "@std/yaml": "jsr:@std/yaml",
    "@clack/prompts": "npm:@clack/prompts",
    "log-update": "npm:log-update",
    "cli-spinners": "npm:cli-spinners"
  }
}
